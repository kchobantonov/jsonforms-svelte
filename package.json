{
  "name": "jsonforms-svelte-monorepo",
  "engines": {
    "node": "^22.0.0",
    "pnpm": ">=10.28.2"
  },
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "example:dev": "pnpm --filter jsonforms-svelte-flowbite-demo dev",
    "wc:dev": "pnpm --filter @chobantonov/jsonforms-svelte-flowbite-webcomponent dev",
    "build": "pnpm -r build",
    "build:libs": "pnpm --filter @chobantonov/jsonforms-svelte build && pnpm --filter @chobantonov/jsonforms-svelte-flowbite build && pnpm --filter @chobantonov/jsonforms-svelte-flowbite-webcomponent build",
    "build:demo": "pnpm --filter jsonforms-svelte-flowbite-demo build",
    "example:preview": "pnpm --filter jsonforms-svelte-flowbite-demo preview",
    "wc:preview": "pnpm --filter @chobantonov/jsonforms-svelte-flowbite-webcomponent preview",
    "clean": "pnpm -r exec rm -rf dist .svelte-kit node_modules",
    "install:all": "pnpm install"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "svelte": "^5.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
