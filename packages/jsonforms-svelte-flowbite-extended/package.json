{
  "name": "@chobantonov/jsonforms-svelte-flowbite-extended",
  "version": "1.0.0",
  "description": "Extended Flowbite-styled JSONForms Svelte 5 renderers",
  "author": "Krasimir Chobantonov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chobantonov/jsonforms-svelte.git"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "npm run check && vite build && npm run prepack",
    "watch": "svelte-kit sync && svelte-package --watch",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "@chobantonov/jsonforms-svelte": "workspace:*",
    "@chobantonov/jsonforms-svelte-flowbite": "workspace:*",
    "@jsonforms/core": "catalog:",
    "ajv": "^8.6.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.10.6",
    "flowbite": "^4.0.1",
    "flowbite-svelte": "^1.31.0",
    "flowbite-svelte-icons": "^3.1.0",
    "lodash": "^4.17.21",
    "maska": "^3.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@chobantonov/jsonforms-svelte": "workspace:*",
    "@chobantonov/jsonforms-svelte-flowbite": "workspace:*",
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^9.39.2",
    "@jsonforms/core": "catalog:",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.50.2",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/lodash": "^4.14.172",
    "@vitest/browser-playwright": "^4.0.18",
    "ajv": "^8.6.1",
    "dayjs": "^1.10.6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^17.3.0",
    "playwright": "^1.58.1",
    "flowbite": "^4.0.1",
    "flowbite-svelte": "^1.31.0",
    "flowbite-svelte-icons": "^3.1.0",
    "lodash": "^4.17.21",
    "maska": "^3.2.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "publint": "^0.3.17",
    "svelte": "^5.49.2",
    "svelte-check": "^4.3.6",
    "tailwindcss": "^4.1.18",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vitest-browser-svelte": "^2.0.2"
  }
}
